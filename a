   <script>
    $(document).ready(function(){
     
     $('#btn_login_details').click(function(){
      
      var error_business_address_name = '';
      var error_business_street_address = '';  
      var error_business_city ='';
      var error_business_state ='';
      var error_business_zip ='';
      var error_business_country = '';
      var error_business_street_address_new = '';
      
      if($.trim($('#business_address_name').val()).length == 0)
      {
       error_business_address_name = 'First Name is required';
       $('#error_business_address_name').text(error_business_address_name);
       $('#business_address_name').addClass('has-error');
      }
      else
      {
       error_business_address_name = '';
       $('#error_business_address_name').text(error_business_address_name);
       $('#business_address_name').removeClass('has-error');
      }
    
      
      if($.trim($('#business_street_address').val()).length == 0)
      {
       error_business_street_address = 'First Name is required';
       $('#error_business_street_address').text(error_business_street_address);
       $('#business_street_address').addClass('has-error');
      }
      else
      {
       error_business_street_address = '';
       $('#error_business_street_address').text(error_business_street_address);
       $('#business_street_address').removeClass('has-error');
      }
    
      
      if($.trim($('#business_city').val()).length == 0)
      {
       error_business_city = 'business_city is required';
       $('#error_business_city').text(error_business_street_address);
       $('#business_city').addClass('has-error');
      }
      else
      {
       error_business_street_address = '';
       $('#error_business_city').text(error_business_street_address);
       $('#business_city').removeClass('has-error');
      }
    
      if($.trim($('#business_state').val()).length == 0)
      {
       error_business_state = 'business_state is required';
       $('#error_business_state').text(error_business_state);
       $('#business_state').addClass('has-error');
      }
      else
      {
       error_business_state = '';
       $('#error_business_state').text(error_business_state);
       $('#business_state').removeClass('has-error');
      }
    
      if($.trim($('#business_zip').val()).length == 0)
      {
       error_business_zip = 'business_zip is required';
       $('#error_business_zip').text(error_business_zip);
       $('#business_zip').addClass('has-error');
      }
      else
      {
       error_business_street_address = '';
       $('#error_business_zip').text(error_business_zip);
       $('#business_zip').removeClass('has-error');
      }
    
      if($.trim($('#business_country').val()).length == 0)
      {
       error_business_country = 'business_country is required';
       $('#error_business_country').text(error_business_country);
       $('#business_country').addClass('has-error');
      }
      else
      {
       error_business_country = '';
       $('#error_business_country').text(error_business_country);
       $('#business_country').removeClass('has-error');
      }
      
      if($.trim($('#business_street_address_new').val()).length == 0)
      {
       error_business_street_address_new = 'business_street_address_new is required';
       $('#error_business_street_address_new').text(error_business_country);
       $('#business_street_address_new').addClass('has-error');
      }
      else
      {
       error_business_street_address_new = '';
       $('#error_business_street_address_new').text(error_business_street_address_new);
       $('#business_street_address_new').removeClass('has-error');
      }
    
      if(error_business_address_name != '' || error_business_street_address != '' || error_business_city != ''|| error_business_state != ''|| error_business_zip != ''|| error_business_country != ''|| error_business_street_address_new != '')
      {
       return false;
      }
      else
      {
       $('#list_login_details').removeClass('active active_tab1');
       $('#list_login_details').removeAttr('href data-toggle');
       $('#login_details').removeClass('active');
       $('#list_login_details').addClass('inactive_tab1');
       $('#list_personal_details').removeClass('inactive_tab1');
       $('#list_personal_details').addClass('active_tab1 active');
       $('#list_personal_details').attr('href', '#personal_details');
       $('#list_personal_details').attr('data-toggle', 'tab');
       $('#personal_details').addClass('active in');
      }
     });
     
     $('#previous_btn_personal_details').click(function(){
      $('#list_personal_details').removeClass('active active_tab1');
      $('#list_personal_details').removeAttr('href data-toggle');
      $('#personal_details').removeClass('active in');
      $('#list_personal_details').addClass('inactive_tab1');
      $('#list_login_details').removeClass('inactive_tab1');
      $('#list_login_details').addClass('active_tab1 active');
      $('#list_login_details').attr('href', '#login_details');
      $('#list_login_details').attr('data-toggle', 'tab');
      $('#login_details').addClass('active in');
     });
     
     $('#btn_personal_details').click(function(){
    
        var error_business_business_type ='';
        var error_business_naics_sector ='';
        var error_business_naics_category ='';
        var error_business_classification ='';
        var error_business_time_zone ='';
      
      if($.trim($('#business_business_type').val()).length == 0)
      {
       error_business_business_type = 'business_business_type name is required';
       $('#error_business_business_type').text(error_business_business_type);
       $('#business_business_type').addClass('has-error');
      }
      else
      {
       error_business_business_type = '';
       $('#error_business_business_type').text(error_business_business_type);
       $('#business_business_type').removeClass('has-error');
      }
      
      if($.trim($('#business_naics_category').val()).length == 0)
      {
       error_business_naics_category = 'business_naics_category name is required';
       $('#error_business_naics_category').text(error_business_naics_category);
       $('#business_naics_category').addClass('has-error');
      }
      else
      {
       error_business_naics_category = '';
       $('#error_business_naics_category').text(error_business_naics_category);
       $('#business_naics_category').removeClass('has-error');
      }
          
      if($.trim($('#business_classification').val()).length == 0)
      {
       error_business_classification = 'business_classification name is required';
       $('#error_business_classification').text(error_business_classification);
       $('#business_classification').addClass('has-error');
      }
      else
      {
       error_business_classification = '';
       $('#error_business_classification').text(error_business_classification);
       $('#business_classification').removeClass('has-error');
      }
                
    
                
      if($.trim($('#business_time_zone').val()).length == 0)
      {
       error_business_time_zone = 'business_time_zone name is required';
       $('#error_business_time_zone').text(error_business_time_zone);
       $('#business_time_zone').addClass('has-error');
      }
      else
      {
       error_business_time_zone = '';
       $('#error_business_time_zone').text(error_business_time_zone);
       $('#business_time_zone').removeClass('has-error');
      }
    
      if(error_business_business_type != '' || error_business_naics_sector != '' || error_business_naics_category != '' || error_business_classification != '' || error_business_time_zone != '')
      {
       return false;
      }
      else
      {
       $('#list_personal_details').removeClass('active active_tab1');
       $('#list_personal_details').removeAttr('href data-toggle');
       $('#personal_details').removeClass('active');
       $('#list_personal_details').addClass('inactive_tab1');
       $('#list_contact_details').removeClass('inactive_tab1');
       $('#list_contact_details').addClass('active_tab1 active');
       $('#list_contact_details').attr('href', '#contact_details');
       $('#list_contact_details').attr('data-toggle', 'tab');
       $('#contact_details').addClass('active in');
      }
     });
     
     $('#previous_btn_contact_details').click(function(){
      $('#list_contact_details').removeClass('active active_tab1');
      $('#list_contact_details').removeAttr('href data-toggle');
      $('#contact_details').removeClass('active in');
      $('#list_contact_details').addClass('inactive_tab1');
      $('#list_personal_details').removeClass('inactive_tab1');
      $('#list_personal_details').addClass('active_tab1 active');
      $('#list_personal_details').attr('href', '#personal_details');
      $('#list_personal_details').attr('data-toggle', 'tab');
      $('#personal_details').addClass('active in');
     });
     
     $('#btn_contact_details').click(function(){
    
        var error_business_first_name = '';
        var error_business_middle_name = '';
        var error_business_last_name = '';
        var error_business_phone = '';
        var error_business_email = '';
    
        if($.trim($('#business_first_name').val()).length == 0)
      {
       error_business_first_name = 'business_first_name is required';
       $('#error_business_first_name').text(error_business_first_name);
       $('#business_first_name').addClass('has-error');
      }
      else
      {
       error_business_first_name = '';
       $('#error_business_first_name').text(error_business_first_name);
       $('#business_first_name').removeClass('has-error');
      }
    
        if($.trim($('#business_middle_name').val()).length == 0)
      {
       error_business_middle_name = 'business_middle_name is required';
       $('#error_business_middle_name').text(error_business_middle_name);
       $('#business_middle_name').addClass('has-error');
      }
      else
      {
       error_business_middle_name = '';
       $('#error_business_middle_name').text(error_business_middle_name);
       $('#business_middle_name').removeClass('has-error');
      }
    
        if($.trim($('#business_last_name').val()).length == 0)
      {
       error_business_last_name = 'business_last_name is required';
       $('#error_business_last_name').text(error_business_last_name);
       $('#business_last_name').addClass('has-error');
      }
      else
      {
       error_business_last_name = '';
       $('#error_business_last_name').text(error_business_last_name);
       $('#business_last_name').removeClass('has-error');
      }
    
        if($.trim($('#business_phone').val()).length == 0)
      {
       error_business_phone = 'business_phone is required';
       $('#error_business_phone').text(error_business_phone);
       $('#business_phone').addClass('has-error');
      }
      else
      {
       error_address = '';
       $('#error_business_phone').text(error_business_phone);
       $('#business_phone').removeClass('has-error');
      }
    
        if($.trim($('#business_email').val()).length == 0)
      {
       error_business_email = 'business_email is required';
       $('#error_business_email').text(error_address);
       $('#business_email').addClass('has-error');
      }
      else
      {
       error_business_email = '';
       $('#error_business_email').text(error_business_email);
       $('#business_email').removeClass('has-error');
      }
     
      if(error_business_first_name != '' || error_business_middle_name != '' || error_business_last_name != '' || error_business_phone != '' || error_business_email != '')
      {
       return false;
      }
      else
      {
       $('#list_contact_details').removeClass('active active_tab1');
       $('#list_contact_details').removeAttr('href data-toggle');
       $('#contact_details').removeClass('active');
       $('#list_contact_details').addClass('inactive_tab1');
       $('#list_login_details_test').removeClass('inactive_tab1');
       $('#list_login_details_test').addClass('active_tab1 active');
       $('#list_login_details_test').attr('href', '#login_details_test');
       $('#list_login_details_test').attr('data-toggle', 'tab');
       $('#login_details_test').addClass('active in');
      }
      
     });
    
